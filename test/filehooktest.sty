%&pdflatex
\ProvidesPackage{filehooktest}
\pagestyle{empty}

\RequirePackage{filehook}[2010/12/29]

\def\test#1{%
  \def\testmessages{}%
}

\newwrite\handle

\def\endtest{%
  \ifx\testmessages\thereference
    \message{^^JRESULT: SUCCESS!^^J}%
  \else
    \message{^^JRESULT: FAILURE: test messages differ from reference^^J}%
    \immediate\openout\handle=\jobname.tmsg\relax
    \immediate\write\handle{\testmessages}%
    \immediate\handle
    \immediate\openout\handle=\jobname.tref\relax
    \immediate\write\handle{\thereference}%
    \immediate\handle
    %\show\testmessages
    %\show\thereference
  \fi
}

\def\testmessage#1{%
    \begingroup
    \edef\@tempa{#1^^J}%
    \expandafter\g@addto@macro
    \expandafter\testmessages
    \expandafter{\@tempa}%
    \printmessage{#1}%
    \endgroup
}

\AtBeginDocument{%
  \print@messages
  \def\printmessage#1{%
    \par\noindent\null#1%
  }%
}

\def\print@messages{}

\def\printmessage#1{%
    \g@addto@macro\print@messages{\par\noindent\null#1}%
}%

\def\reference{%
    \begingroup
    \endlinechar=\newlinechar
    \reference@
}

\def\reference@#1{%
    \endgroup
    \def\thereference{#1}%
}

\endinput
